@model  CangooruMVC.Models.OrderModel
@{
    ViewBag.Title = "Index";
}
<link rel="stylesheet" media="all" type="text/css" href= "@Url.Content("Scripts/DateTimePicker/jquery-ui-timepicker-addon.css")" />
<script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.23.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/DateTimePicker/jquery-ui-timepicker-addon.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/DateTimePicker/jquery-ui-sliderAccess.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.maskedinput-1.3.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/JqEasyCharCounter/jquery.jqEasyCharCounter.js")" type="text/javascript"></script>
<div id="tabs">
    <nav>
        <ul>
            <li>@Html.ActionLink("Nova Entrega", "Create", "Order")</li>
            <li>@Html.ActionLink("Minhas Localidades", "Index", "Locations", new { showEditColumns = false }, null)</li>
            <li>@Html.ActionLink("Minhas Entregas", "GetOrders", "Order")</li>
            <li>@Html.ActionLink("Minha Conta", "MyAccount", "Account")</li>
        </ul>
    </nav>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $("#tabs").tabs({
            ajaxOptions: {
                error: function (xhr, status, index, anchor) {
                    $(anchor.hash).html(
						"Couldn't load this tab. We'll try to fix this as soon as possible. " +
						"If this wouldn't be a demo.");
                },
                select: function (event, ui) {
                    $("#modalDiv:ui-dialog").dialog("destroy");
                }
            }
        });
    });
</script>
